<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dochadzka"
        xmlns:Data="clr-namespace:System.Data;assembly=System.Data.Common" x:Name="wndMain" x:Class="dochadzka.MainWindow"
        mc:Ignorable="d"
        Title="Dochádzka" Height="600" Width="900" Loaded="Window_Loaded" Closed="Window_Closed" MinHeight="600" MinWidth="900" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid>
        <!--<Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>-->
        <TabControl x:Name="tcMain" Grid.Row="0" SelectionChanged="tcMain_SelectionChanged">
            <TabItem x:Name="tiZamestnanec" Header="Zamestnanec" IsSelected="True">
                <Grid>
                    <Grid x:Name="grdZostatky" Visibility="Visible">
                        <Label x:Name="lblZostatky" Content="Zostatky:" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                        <Label x:Name="lblDovolenka" Content="Dovolenka:" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="lblDovolenkaZ" HorizontalAlignment="Left" Margin="91,36,0,0" Content="XX.X dni" VerticalAlignment="Top" Width="57"/>
                        <Label x:Name="lblLekar" Content="Lekár:" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="lblLekarZ" HorizontalAlignment="Left" Margin="91,67,0,0" Content="XX h" VerticalAlignment="Top" Width="47"/>
                        <Label x:Name="lblLekarDoprovod" Content="Lekár dopr.:" ToolTip="Lekár doprovod" HorizontalAlignment="Left" Margin="10,98,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="lblLekarDoprovodZ" HorizontalAlignment="Left" Margin="91,98,0,0" Content="XX h" VerticalAlignment="Top" Width="47"/>
                        <Label x:Name="lblLekarDoprovodZP" Content="Lekár d. z. p.:" ToolTip="Lekár doprovod zdravotne postihnutého" HorizontalAlignment="Left" Margin="10,129,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="lblLekarDoprovodZPZ" HorizontalAlignment="Left" Margin="91,129,0,0" Content="XX h" VerticalAlignment="Top" Width="47"/>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem x:Name="tiData" Header="Dáta" IsEnabled="False">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="270"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <DataGrid  x:Name="dgDochadzka" Grid.Column="0" Grid.RowSpan="5" d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Stretch" 
							   ClipboardCopyMode="IncludeHeader" Margin="0,0,10,0" VerticalAlignment="Stretch" AlternatingRowBackground="LightGray"
							   RowEditEnding="dgDochadzka_RowEditEnding" AutoGenerateColumns="False" CellEditEnding="dgDochadzka_CellEditEnding" CanUserReorderColumns="False" 
							   PreviewKeyDown="dgDochadzka_PreviewKeyDown" HeadersVisibility="Column" SelectionChanged="dgDochadzka_SelectionChanged" BeginningEdit="dgDochadzka_BeginningEdit"
                               
                               >
                        <!--<DataGrid.RowValidationRules>
                            <local:DB_t_UserValidationRule ValidationStep="UpdatedValue"/>
                        </DataGrid.RowValidationRules>-->

                        <DataGrid.RowStyle>
                            <Style TargetType="DataGridRow">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding Lock}"  Value="true">
                                        <Setter Property="Foreground" Value="DarkRed"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.RowStyle>
                        <DataGrid.Columns>
                            <DataGridCheckBoxColumn x:Name="dgcLock" Header="Lock" IsReadOnly="True" Binding="{Binding Lock}" IsThreeState="False" Visibility="Hidden"/>
                            <DataGridTextColumn x:Name="dgcSVK" Header="SVK" IsReadOnly="True" Binding="{Binding CountryCode}" Visibility="Hidden"/>
                            <DataGridTextColumn x:Name="dgcID" Header="ID" IsReadOnly="True" Binding="{Binding ID}" Visibility="Hidden"/>
                            <DataGridComboBoxColumn x:Name="dgcCinnostTyp" Header="Typ činnosti" SelectedValueBinding="{Binding CinnostTypID, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <DataGridTemplateColumn x:Name="dgcStartDate" Header="Dátum" SortMemberPath="StartDate">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding StartDate, StringFormat=dd.MM.yyyy}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                                <DataGridTemplateColumn.CellEditingTemplate>
                                    <DataTemplate>
                                        <DatePicker SelectedDate ="{Binding StartDate, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellEditingTemplate>
                            </DataGridTemplateColumn>
                            <!--<DataGridComboBoxColumn x:Name="dgcProject" Header="Project" SelectedValueBinding="{Binding ProjectID, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>-->
                            <DataGridTemplateColumn x:Name="dgcProject" Header="Projekt" SortMemberPath="NumberName">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding NumberName}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                                <DataGridTemplateColumn.CellEditingTemplate>
                                    <DataTemplate>
                                        <ComboBox x:Name="cbProject" Loaded="ComboBox_Loaded" Text="{Binding NumberName}" SelectedValuePath="{Binding NumberName, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" SelectedValue="{Binding ProjectID, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellEditingTemplate>
                            </DataGridTemplateColumn>
                            <DataGridComboBoxColumn x:Name="dgcHourType" Header="Produktivita" SelectedValueBinding="{Binding HourTypeID, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <DataGridComboBoxColumn x:Name="dgcHourTypes" Header="Druh činnosti" SelectedValueBinding="{Binding HourTypesID, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <!--<DataGridComboBoxColumn x:Name="dgcEndTime" Header="EndTime" SelectedValueBinding="{Binding EndTime, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, StringFormat=hh\\:mm}" TextBinding="{Binding EndTime, StringFormat=hh\\:mm}"/>-->
                            <DataGridTemplateColumn x:Name="dgcStartTime" Header="Začiatok" SortMemberPath="StartTime">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding StartTime, StringFormat=hh\\:mm}" HorizontalAlignment="Center"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                                <DataGridTemplateColumn.CellEditingTemplate>
                                    <DataTemplate>
                                        <ComboBox x:Name="cbStartTime" SelectionChanged="ComboBox_SelectionChanged" Loaded="ComboBox_Loaded" Text="{Binding StartTime, StringFormat=hh\\:mm}" SelectedValue="{Binding StartTime, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                        <!--SelectedItem="{Binding StartTime, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>-->
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellEditingTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn x:Name="dgcEndTime" Header="Koniec" SortMemberPath="EndTime">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding EndTime, StringFormat=hh\\:mm}" HorizontalAlignment="Center"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                                <DataGridTemplateColumn.CellEditingTemplate>
                                    <DataTemplate>
                                        <ComboBox x:Name="cbEndTime" Loaded="ComboBox_Loaded" Text="{Binding EndTime, StringFormat=hh\\:mm}" SelectedValue="{Binding EndTime, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellEditingTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn Header="Hodín" IsReadOnly="True" Binding="{Binding WorkTime.TotalHours, StringFormat=N1}">
                                <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="HorizontalAlignment" Value="Right"/>
                                        <Setter Property="Margin" Value="0,0,2,0"/>
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn x:Name="dgcDescription" Header="Popis" Width="*" Binding="{Binding Description, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <DataGridTextColumn x:Name="dgcKM" Header="km" Binding="{Binding km, NotifyOnSourceUpdated=True, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="HorizontalAlignment" Value="Right"/>
                                        <Setter Property="Margin" Value="0,0,2,0"/>
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridCheckBoxColumn x:Name="dgcDlhodobaZSC" Header="Dlhodobá ZSC" Binding="{Binding DlhodobaSC, UpdateSourceTrigger=PropertyChanged}" IsThreeState="False">
                                <!--NotifyOnSourceUpdated=True, Mode=TwoWay,-->
                                <DataGridCheckBoxColumn.ElementStyle>
                                    <Style TargetType="CheckBox" BasedOn="{x:Static DataGridCheckBoxColumn.DefaultElementStyle}">
                                        <Setter Property="HorizontalAlignment" Value="Center"/>
                                        <Setter Property="Visibility" Value="Visible"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding CountryCode}" Value="SVK">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding CountryCode}" Value="{x:Null}">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding HourTypeID}" Value="1">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding HourTypeID}" Value="4">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGridCheckBoxColumn.ElementStyle>
                                <DataGridCheckBoxColumn.EditingElementStyle>
                                    <Style TargetType="CheckBox" BasedOn="{x:Static DataGridCheckBoxColumn.DefaultEditingElementStyle}">
                                        <Setter Property="HorizontalAlignment" Value="Center"/>
                                        <Setter Property="Visibility" Value="Visible"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding CountryCode}" Value="SVK">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding CountryCode}" Value="{x:Null}">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding HourTypeID}" Value="1">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding HourTypeID}" Value="4">
                                                <Setter Property="Visibility" Value="Hidden"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGridCheckBoxColumn.EditingElementStyle>
                            </DataGridCheckBoxColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    
                    <Grid x:Name="grdZamestnanecPravaStrana" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Right" Height="100" VerticalAlignment="Top" Width="180"
						  Visibility="Visible">
                        <Border BorderThickness="1" BorderBrush="Black" Width="180" Height="100"/>
                        <Label x:Name="lblDateFrom" Content="Od:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
                        <DatePicker x:Name="dpDateFrom" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="40,10,10,0" 
									SelectedDateChanged="DatePicker_SelectedDateChanged" FirstDayOfWeek="Monday"/>
                        <Label x:Name="lblDateTo" Content="Do:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,40,0,0"/>
                        <DatePicker x:Name="dpDateTo" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="40,40,10,0" 
									SelectedDateChanged="DatePicker_SelectedDateChanged" FirstDayOfWeek="Monday"/>
                        <CheckBox x:Name="chkCelyMesiac" Content="Celý mesiac"  Margin="40,73,40,10" 
								  Checked="chkCelyMesiac_CheckChanged" Unchecked="chkCelyMesiac_CheckChanged"/>
                    </Grid>
                    <Grid x:Name="grdSZCOPravaStrana" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Right" Height="100" VerticalAlignment="Top" Width="180"
						  Visibility="Hidden">
                        <Border BorderThickness="1" BorderBrush="Black" Width="180" Height="100"/>
                        <Label Content="Rok:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
                        <ComboBox x:Name="cboSZCORok" Height="20" Width="90"  VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,13,10,0" />
                        <Label Content="Mesiac:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,41,0,0"/>
                        <ComboBox x:Name="cboSZCOMesiac" Height="20" Width="90"  VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,44,10,0" />
                    </Grid>
                    <Button x:Name="btnLoadData" Grid.Column="1" Grid.Row="0" Content="Načítaj dáta" Click="btnLoadData_Click" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,110,10,0"/>

                    <Grid Grid.Column="1" Grid.Row="1" x:Name="grdShownUser">
                        <Label Grid.Column="1" x:Name="lblShownUser" Content="Zobrazená dochádzka pre:" HorizontalAlignment="Left" Margin="10,15,0,0" VerticalAlignment="Top"/>
                        <ComboBox x:Name="cboShownUser" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="10,40,10,0"
                              SelectionChanged="cboShownUser_SelectionChanged" />
                    </Grid>

                    <GroupBox x:Name="gbKopirovanie" Grid.Column="1" Grid.Row="2" 
							  Header="Kopírovanie označených riadkov" 
							  Margin="0,15,0,0" IsEnabled="False"
							  ><!--&#xA;-->
                        <Grid VerticalAlignment="Top">
                            <RadioButton Content="Pridať so zachovaním dní" Height="20"
										 x:Name="rbPridatSoZachovanim"
										 ToolTip="napr. &#xA;pondelok -> pondelok &#xA;streda -> streda &#xA;piatok -> piatok" 
										 HorizontalAlignment="Left" VerticalAlignment="Top"
										 Margin="0,10,10,0" IsChecked="True" Checked="rbPridatSoZachovanim_Checked"/>
                            <RadioButton Content="Pridať bez zachovania dní" Height="20"
										 x:Name="rbPridatBezZachovania"
										 ToolTip="napr. &#xA;pondelok -> pondelok &#xA;streda -> utorok &#xA;piatok -> streda" 
										 HorizontalAlignment="Left" VerticalAlignment="Top"
										 Margin="0,30,10,0" Checked="rbPridatBezZachovania_Checked"/>
                            <Label Content="Od:" Margin="3,53,0,0" 
								   HorizontalAlignment="Left" VerticalAlignment="Top"
								   />
                            <DatePicker x:Name="dpDataKopirovanie" Margin="0,55,10,10"
										HorizontalAlignment="Right" VerticalAlignment="Top"
										Height="25" />
                            <Button x:Name="btnDataKopirovanie" Content="Kopírovať" Height="20" Width="100" 
									HorizontalAlignment="Right" VerticalAlignment="Top"
									Margin="0,95,10,0" Click="btnDataKopirovanie_Click"/>
                            <Image x:Name="imgKopirovaniePriklad" Visibility="Visible"
								   VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,130,10,0"
								   Width="150" Height="100" Source="img/so.zachovanim.dna.png"
								   />
                            <Button x:Name="btnDataCSV" Content="Ulož CSV" Margin="0,240,8,0"
                                    HorizontalAlignment="Right" Click="btnDataCSV_Click"
                                    />
                        </Grid>
                    </GroupBox>
 
                    <GroupBox x:Name="gbDataSumar" Grid.Column="1" Grid.Row="3" Margin="0,15,0,0" MaxHeight="300"
                              Header="Sumár označených dát" BorderBrush="Black" BorderThickness="1"
							  VerticalAlignment="Top">
                        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
                            <Grid x:Name="grdDataSumar" VerticalAlignment="Top">
                                <!--<Label x:Name="lblDataSumarProjekt" Content="DataSumarProjekt"
                               HorizontalAlignment="Left" Margin="10,10,0,0"/>
                                <Label x:Name="lblDataSumarCas" Content="DataSumarCas"
                               HorizontalAlignment="Left" Margin="10,40,0,0"/>-->
                            </Grid>
                        </ScrollViewer>
                    </GroupBox>
               </Grid>
            </TabItem>
            <TabItem x:Name="tiNadcas" Header="Nadčas" IsEnabled="False">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="180"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="170"/>
                        <RowDefinition Height="170"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid x:Name="grdNadcasData" Grid.Column="0" Grid.RowSpan="3"
                          Visibility="Visible">
                        <Grid.Resources>
                            <local:TimeSpanConverter x:Key="timeSpanConverter"/>
                        </Grid.Resources>
                        <DataGrid x:Name="dgNadcas" d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                                  ClipboardCopyMode="IncludeHeader" Margin="0,0,10,0" AlternatingRowBackground="LightGray"
                                  AutoGenerateColumns="False" CanUserReorderColumns="False" HeadersVisibility="Column"  
                                  IsReadOnly="True" CanUserAddRows="False" CanUserDeleteRows="False" 
                                  SelectionChanged="dgNadcas_SelectionChanged">
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="Dátum" Binding="{Binding Datum, StringFormat=dd.MM.yyyy}"/>
                                <DataGridTextColumn Header="Nadčas" Binding="{Binding Nadcas, Converter={StaticResource timeSpanConverter}}">
                                    <DataGridTextColumn.ElementStyle>
                                        <Style TargetType="TextBlock">
                                            <Setter Property="HorizontalAlignment" Value="Right"/>
                                            <Setter Property="Margin" Value="0,0,20,0"/>
                                        </Style>
                                    </DataGridTextColumn.ElementStyle>
                                </DataGridTextColumn>
                                <DataGridTextColumn Header="Typ" Binding="{Binding Typ}"/>
                                <DataGridTextColumn x:Name="dgcNadcasSchvalil" Header="Schválil" Binding="{Binding FullName}"/>
                                <DataGridTextColumn Header="Poznámka" Binding="{Binding Poznamka}" Width="*"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                    <Grid x:Name="grdNadcasPrint" Grid.Column="0" Grid.RowSpan="3"
                          Visibility="Hidden">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="40"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Grid.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FFA9A9BF" Offset="1"/>
                                    <GradientStop Color="White"/>
                                </LinearGradientBrush>
                            </Grid.Background>
                            <Button x:Name="btnNPrint" Content="Print" HorizontalAlignment="Left" Margin="65,10,0,0" VerticalAlignment="Top" Width="50" Click="btnPrint_Click" />
                            <Button x:Name="btnNLeft" Content="&lt;" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="20" Click="btnNextPageVisible_Click"/>
                            <Button x:Name="btnNRight" Content="&gt;" HorizontalAlignment="Left" Margin="31,10,0,0" VerticalAlignment="Top" Width="20" Click="btnNextPageVisible_Click"/>
                        </Grid>
                        <ScrollViewer Grid.Row="1">
                            <Grid>
                                <Grid x:Name="grdNadcasPrintArea" Width="400" HorizontalAlignment="Left" Margin="10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="60"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Label Grid.Row="0" Content="Výkaz nadčasov za obdobie" FontSize="15" HorizontalAlignment="Center"/>
                                    <Label Grid.Row="1" x:Name="lblNadcasHeader" Content="01.01.2023 - 31.01.2023" FontSize="13" FontWeight="Bold" HorizontalAlignment="Center"/>
                                    <DataGrid x:Name="dgNadcasPrint" Grid.Row="2" Width="200" 
                                              d:ItemsSource="{d:SampleData ItemCount=4}" HeadersVisibility="Column" 
                                              IsReadOnly="True" IsHitTestVisible="False" HorizontalAlignment="Center"
                                              BorderBrush="Black" BorderThickness="2" AutoGenerateColumns="False" 
                                              HorizontalScrollBarVisibility="Disabled" Margin="0,0,-1,0"
                                              >
                                        <DataGrid.ColumnHeaderStyle>
                                            <Style TargetType="DataGridColumnHeader">
                                                <Setter Property="Background" Value="DarkGray"/>
                                                <Setter Property="BorderThickness" Value="1.5"/>
                                                <Setter Property="BorderBrush" Value="Black"/>
                                                <Setter Property="Margin" Value="-1,-1,-1,0"/>
                                                <Setter Property="FontWeight" Value="Bold"/>
                                            </Style>
                                        </DataGrid.ColumnHeaderStyle>
                                        <DataGrid.Columns>
                                            <DataGridTextColumn Width="100" Header="Typ nadčasu" Binding="{Binding Item1}"/>
                                            <DataGridTextColumn Width="100" Header="Počet hodín" Binding="{Binding Item2, StringFormat=N2}">
                                                <DataGridTextColumn.ElementStyle>
                                                    <Style TargetType="TextBlock">
                                                        <Setter Property="HorizontalAlignment" Value="Right"/>
                                                        <Setter Property="Margin" Value="0,0,35,0"/>
                                                    </Style>
                                                </DataGridTextColumn.ElementStyle>
                                            </DataGridTextColumn>
                                        </DataGrid.Columns>
                                    </DataGrid>
                                </Grid>
                            </Grid>
                        </ScrollViewer>
                    </Grid>
                    <Grid x:Name="grdNadcasPravaStrana" Grid.Column="1" Grid.Row="0" Height="130" Width="180"
                          HorizontalAlignment="Right" VerticalAlignment="Top" 
						  Visibility="Visible">
                        <Border BorderThickness="1" BorderBrush="Black" />
                        <Label x:Name="lblNadcasDateFrom" Content="Od:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
                        <DatePicker x:Name="dpNadcasDateFrom" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="40,10,10,0" 
									SelectedDateChanged="DatePicker_SelectedDateChanged" FirstDayOfWeek="Monday"/>
                        <Label x:Name="lblNadcasDateTo" Content="Do:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,40,0,0"/>
                        <DatePicker x:Name="dpNadcasDateTo" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="40,40,10,0" 
									SelectedDateChanged="DatePicker_SelectedDateChanged" FirstDayOfWeek="Monday"/>
                        <CheckBox x:Name="chkNadcasCelyMesiac" Content="Celý mesiac"  Margin="40,73,40,10" 
								  Checked="chkNadcasCelyMesiac_CheckChanged" Unchecked="chkNadcasCelyMesiac_CheckChanged"/>
                        <Label Content="Typ:" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,0,0,10"/>
                        <ComboBox x:Name="cboNadcasTyp" Height="20" Width="130" 
                                  VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,10,10" 
                                  />
                    </Grid>
                    <Button x:Name="btnNadcasTlacit" Content="Tlačiť" Grid.Column="1" Grid.Row="0" 
                            HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10"
                            Click="btnNadcasTlacit_Click"/>
                    <Button x:Name="btnNadcasCSV" Content="Ulož _CSV" Grid.Column="1" Grid.Row="0" 
                            HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="10"
                            Click="btnNadcasCSV_Click"/>
                    <Button x:Name="btnNadcasObnov" Content="Obnov" Grid.Column="1" Grid.Row="0" 
                            HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10" 
                            Click="btnNadcasObnov_Click"/>
                    <Grid x:Name="grdNadcasSumar" Grid.Column="1" Grid.Row="1" Height="140" Width="180"
                          HorizontalAlignment="Left" VerticalAlignment="Top">
                        <Border x:Name="borNadcasSumar" BorderThickness="1" BorderBrush="Black" />
                        <Label x:Name="lblNadcasSumarFlexi" Content="NadcasSumarFlexi"
                               HorizontalAlignment="Left" Margin="10,10,0,0"/>
                        <Label x:Name="lblNadcasSumarSCSKCesta" Content="NadcasSumarSCSKCesta"
                               HorizontalAlignment="Left" Margin="10,40,0,0"/>
                        <Label x:Name="lblNadcasSumarSCZahranicie" Content="NadcasSumarSCZahranicie"
                               HorizontalAlignment="Left" Margin="10,70,0,0"/>
                        <Label x:Name="lblNadcasSumarNeplateny" Content="NadcasSumarNeplateny"
                               HorizontalAlignment="Left" Margin="10,100,0,0"/>
                    </Grid>
                    <Grid x:Name="grdLock" Grid.Column="1" Grid.Row="2" Height="90" Width="180" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <Border BorderThickness="2" BorderBrush="Red" />
                        <TextBlock x:Name="tblLock" Text="TextBlock" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                               Margin="10" Width="160" TextWrapping="WrapWithOverflow" FontStretch="Expanded" Foreground="DarkRed"/>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem x:Name="tiVykaz" Header="Výkaz" IsEnabled="False" >
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
