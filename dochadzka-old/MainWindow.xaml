                                <DataGrid x:Name="dgProjekty" Margin="10,45,10,10" VerticalAlignment="Top" 
										  AutoGenerateColumns="False" AlternatingRowBackground="LightGray"
										  RowEditEnding="dgProjekty_RowEditEnding" HeadersVisibility="Column" BorderBrush="Black"
										  d:ItemsSource="{d:SampleData ItemCount=5}" PreviewKeyDown="dgProjekty_PreviewKeyDown">
