                                        <DataGridTextColumn Header="ID" Binding="{Binding ID}" IsReadOnly="True"/>
                                        <DataGridTextColumn Header="Meno" Binding="{Binding Name, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Číslo" Binding="{Binding Number, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridTextColumn Header="Popis" Binding="{Binding Description, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridCheckBoxColumn Header="Pridelovanie hodín" Binding="{Binding AllowAssignWorkingHours, UpdateSourceTrigger=PropertyChanged}" IsThreeState="False"/>
                                        <DataGridComboBoxColumn x:Name="dgcCountry" Header="Krajina" SelectedValueBinding="{Binding CountryCode, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}"/>
                                        <DataGridComboBoxColumn x:Name="dgcManager" Header="Manažér" Width="*" SelectedValueBinding="{Binding Manager, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}"/>
